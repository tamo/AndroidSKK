<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/popup_frame"
	android:padding="2dp"
	android:layout_width="120dp"
	android:layout_height="120dp">

	<TextView
		android:id="@+id/labelA"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:layout_centerInParent="true"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="center"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="22sp"
		android:maxLines="1"
		android:autoSizeMaxTextSize="22sp"
		android:autoSizeMinTextSize="8sp"
		android:autoSizeStepGranularity="1sp"
		android:autoSizeTextType="uniform" />

	<TextView
		android:id="@+id/labelI"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:layout_alignTop="@id/labelA"
		android:layout_toLeftOf="@id/labelA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="center"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="22sp"
		android:maxLines="1"
		android:autoSizeMaxTextSize="22sp"
		android:autoSizeMinTextSize="8sp"
		android:autoSizeStepGranularity="1sp"
		android:autoSizeTextType="uniform" />

	<TextView
		android:id="@+id/labelU"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:layout_above="@id/labelA"
		android:layout_alignLeft="@id/labelA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="center"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="22sp"
		android:maxLines="1"
		android:autoSizeMaxTextSize="22sp"
		android:autoSizeMinTextSize="8sp"
		android:autoSizeStepGranularity="1sp"
		android:autoSizeTextType="uniform" />

	<TextView
		android:id="@+id/labelE"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:layout_alignTop="@id/labelA"
		android:layout_toRightOf="@id/labelA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="center"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="22sp"
		android:maxLines="1"
		android:autoSizeMaxTextSize="22sp"
		android:autoSizeMinTextSize="8sp"
		android:autoSizeStepGranularity="1sp"
		android:autoSizeTextType="uniform" />

	<TextView
		android:id="@+id/labelO"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:layout_below="@id/labelA"
		android:layout_alignRight="@id/labelA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="center"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="22sp"
		android:maxLines="1"
		android:autoSizeMaxTextSize="22sp"
		android:autoSizeMinTextSize="8sp"
		android:autoSizeStepGranularity="1sp"
		android:autoSizeTextType="uniform" />

	<TextView
		android:id="@+id/labelLeftA"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_above="@id/labelA"
		android:layout_toLeftOf="@id/labelA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelRightA"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_above="@id/labelA"
		android:layout_toRightOf="@id/labelA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelLeftI"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_below="@id/labelI"
		android:layout_toLeftOf="@id/labelLeftA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelRightI"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_above="@id/labelI"
		android:layout_toLeftOf="@id/labelLeftA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelLeftU"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_above="@id/labelLeftA"
		android:layout_toLeftOf="@id/labelU"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelRightU"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_above="@id/labelRightA"
		android:layout_toRightOf="@id/labelU"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelLeftE"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_above="@id/labelE"
		android:layout_toRightOf="@id/labelRightA"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelRightE"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_below="@id/labelE"
		android:layout_toRightOf="@id/labelLeftO"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelLeftO"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_below="@id/labelLeftI"
		android:layout_toRightOf="@id/labelO"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />

	<TextView
		android:id="@+id/labelRightO"
		android:layout_width="16dp"
		android:layout_height="16dp"
		android:layout_below="@id/labelLeftI"
		android:layout_toLeftOf="@id/labelO"
		android:background="@drawable/popup_label"
		android:fallbackLineSpacing="false"
		android:gravity="top|left"
		android:includeFontPadding="false"
		android:textColor="@color/key_char_color"
		android:textSize="12sp" />
</RelativeLayout>

